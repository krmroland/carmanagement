    "license": "ISC",
    "devDependencies": {
        "@babel/preset-react": "^7.0.0",
        "@prettier/plugin-php": "^0.12.0",
        "cross-env": "^7.0.0",
        "laravel-mix": "^4.0.14",
        "laravel-mix-purgecss": "^4.1",
        "laravel-mix-tailwind": "^0.1.0",
        "prettier": "^1.19.1",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.25.0",
        "sass-loader": "^8.0.2",
        "tailwindcss": "^1.0",
